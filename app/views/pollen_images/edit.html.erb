<%- model_class = PollenImage -%>
<div class="page-header">
  <h1><%=t '.title', :default => [:'helpers.titles.edit', 'Edit %{model}'], :model => @pollen_image.title  %></h1>
</div>
<%= form_for @pollen_image, :url => pollen_images_path, :html => { :multipart => true } do |form| %>
  <%= form.file_field :image %>
<% end %>
<%= render :partial => 'form' %>